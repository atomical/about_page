<div class="about_page">
<h1>Dependency Versions</h1>
<div class="dependencies">
  <ul>
    <% @dependencies.each do |spec, version| %>
      <li><b><%= spec %></b> (<%= version %>)</li>
    <% end %>
  </ul>
</div>

<h1>Fedora</h1>
<ul>
  <% @fedora_profile.each_pair do |k,v| %>
    <li><%= [k,v].join(': ') %></li>
  <% end %>
</ul>

<h1>Solr Index</h1>
<ul>
  <% @solr_profile.each_pair do |k,v| %>
    <li><%= [k,v].join(': ') %></li>
  <% end %>
</ul>

<h1>Environment</h1>
<% @request_env.each_pair do |section,env| %>
  <h3><%= section %></h3>
  <div class="environment">
    <ul>
      <% env.keys.sort.each do |key| %>
      <li><%= [key,env[key]].join('=') %></li>
      <% end %>
    </ul>
  </div>
<% end %>
</div>